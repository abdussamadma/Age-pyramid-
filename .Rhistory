library(tidycensus)
library(cowplot)
library(tidycensus)
library(tidyverse)
library(tidyselect)
library(tidyr)
wisconsin <- get_decennial(
geography = "state",
state = "55",
product = "characteristics",
breakdown = c("SEX", "AGEGROUP"),
breakdown_labels = TRUE,
)
wisconsin
wisconsin <- get_estimates(
geography = "state",
state = "55",
product = "characteristics",
breakdown = c("SEX", "AGEGROUP"),
breakdown_labels = TRUE,
)
wisconsin
View(wisconsin)
View(wisconsin)
View(wisconsin)
View(wisconsin)
wisconsin_filtered <- filter(wisconsin, str_detect(AGEGROUP, "^Age"),
SEX != "Both sexes") %>%
mutate(value = ifelse(SEX == "Male", -value, value))
wisconsin_filtered
View(wisconsin_filtered)
View(wisconsin_filtered)
ggplot(wisconsin_filtered, aes(x = value, y = AGEGROUP, fill = SEX)) +
geom_col()
library(plotly)
##---first-pyramid------
wisconsin_pyramid <- ggplot(wisconsin_filtered, aes(x = value, y = AGEGROUP, fill = SEX)) +
geom_col()
ggplotly(wisconsin_pyramid)
